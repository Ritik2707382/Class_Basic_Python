        "class Employee:\n",
        "  def __init__(self,name,salary):\n",
        "    self.name=name\n",
        "    self.salary=salary\n",
        "  def raise_salary(self,percentage):\n",
        "    self.salary=self.salary+self.salary*(percentage/100)\n",
        "e1=Employee(\"h\",2000)\n",
        "e1.raise_salary(15)"
