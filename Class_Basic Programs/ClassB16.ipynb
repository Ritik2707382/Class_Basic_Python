        "class Student:\n",
        "  def __init__(self,name,grades=[]):\n",
        "    self.name=name\n",
        "    self.grades=grades\n",
        "  def cal_avg(self):\n",
        "    average=sum(self.grades)/len(self.grades)\n",
        "    print(\"The Average of Grades are:\",average)\n",
        "s1=Student(\"Alice\",[85,90,95])\n",
        "s1.cal_avg()"
      ]
